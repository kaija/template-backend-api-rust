# Environment configuration example
# Copy this file to .env and update the values

# Application environment
ENVIRONMENT=development

# Database configuration
APP_DATABASE__URL=postgresql://username:password@localhost/rust_api_template

# Sentry configuration
APP_SENTRY__DSN=https://your-sentry-dsn@sentry.io/project-id

# Vault configuration (optional)
APP_VAULT__ADDRESS=http://localhost:8200
APP_VAULT__TOKEN=your-vault-token

# Server configuration
APP_SERVER__HOST=0.0.0.0
APP_SERVER__PORT=8080

# Logging configuration
APP_LOGGING__LEVEL=debug